        "description": {
          "version": 1,
          "type": "doc",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "User Stories"
                }
              ]
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "As a customer "
                }
              ]
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "I want to be able to add my selected configuration of a product to my basket "
                }
              ]
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "so that I can progress with my purchase"
                }
              ]
            },
            {
              "type": "paragraph",
              "content": []
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "As a customer "
                }
              ]
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "When I add my selected configuration to my basket (new or existing) "
                }
              ]
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "I want any stock checks to be performed against the specific configuration i have chosen "
                }
              ]
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "so that I can be sure that the  quantity that I have specified is available"
                }
              ]
            },
            {
              "type": "paragraph",
              "content": []
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Logged in only"
                }
              ]
            },
            {
              "type": "paragraph",
              "content": []
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "new basket and existing basket "
                }
              ]
            },
            {
              "type": "paragraph",
              "content": []
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "As a customer I want to be able to see a really simple view of the product so that I can add to basket."
                }
              ]
            },
            {
              "type": "paragraph",
              "content": []
            },
            {
              "type": "paragraph",
              "content": []
            },
            {
              "type": "paragraph",
              "content": []
            },
            {
              "type": "paragraph",
              "content": []
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Acceptance Criteria",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "As a user I expect all ATB functionality to operate as it does for other product types."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "paragraph",
              "content": []
            },
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "need to be specific"
                }
              ]
            },
            {
              "type": "paragraph",
              "content": []
            }
          ]
